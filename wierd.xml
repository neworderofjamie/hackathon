<?xml version="1.0"?>
<SpineML xmlns="http://nineml.org/9ML/0.1">
 <LayoutClass name="wierd">
  <Parameter name="numNeurons" dimension="?"/>
  <Parameter name="length" dimension="?"/>
  <Parameter name="space" dimension="?"/>
  <Parameter name="curve" dimension="?"/>
  <Parameter name="dist" dimension="?"/>
  <Parameter name="width" dimension="?"/>
  <Spatial>
   <Regime name="">
    <Transform variable="z" order="2" dimension="??" type="translate">
     <MathInline>mod(t,length/2)*space</MathInline>
    </Transform>
    <Transform variable="y" order="3" dimension="??" type="translate">
     <MathInline>floor(t/length)</MathInline>
    </Transform>
    <Transform variable="t" order="5" dimension="??" type="translate">
     <MathInline>t+1</MathInline>
    </Transform>
    <Transform variable="x" order="4" dimension="??" type="translate">
     <MathInline>floor(temp/length/2)*dist</MathInline>
    </Transform>
    <Transform variable="temp" order="1" dimension="??" type="translate">
     <MathInline>mod(t,length)</MathInline>
    </Transform>
   </Regime>
   <StateVariable name="x" dimension="?"/>
   <StateVariable name="y" dimension="?"/>
   <StateVariable name="z" dimension="?"/>
   <StateVariable name="t" dimension="?"/>
   <StateVariable name="temp" dimension="?"/>
  </Spatial>
 </LayoutClass>
</SpineML>
